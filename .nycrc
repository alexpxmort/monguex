{
  "extends": "@istanbuljs/nyc-config-typescript",
  "sourceMap": true,
  "instrument": true,
  "all": true,
  "check-coverage": true,
  "lines": 60,
  "statements": 60,
  "branches": 60,
  "functions": 60,
  "report-dir": "./report/coverage",
  "reporter": ["html", "text", "lcov"],
  "include": ["src/**/*.ts"],
  "extension": [".ts"],
  "exclude": ["**/node_modules/**", "test/**"]
}
