{
  "name": "monguex",
  "version": "1.0.6",
  "main": "build/lib/index.js",
  "types": "build/lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexpxmort/monguex.git"
  },
  "keywords": [
    "mongoose",
    "mongo",
    "framework"
  ],
  "author": {
    "name": "Alex Oliveira",
    "email": "lexpdigi@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "format": "eslint --fix .",
    "test:coverage": "nyc yarn test",
    "test": "mocha -r tsconfig-paths/register --no-timeout --exit --config test/runners/mocha/.mocharc.json --file test/dbHelper.ts 'test/**/**.spec.ts'",
    "build": "tsc",
    "dev": "ts-node-dev -r tsconfig-paths/register  --inspect --respawn --transpile-only --ignore-watch node_modules src/index.ts"
  },
  "dependencies": {
    "mongoose": "^6.1.3"
  },
  "devDependencies": {
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2",
    "@swc-node/register": "1.4.2",
    "@swc/helpers": "0.3.8",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "mocha": "^9.1.3",
    "chai": "^4.3.4",
    "mongodb-memory-server": "^8.2.0",
    "@types/sinon": "^10.0.6",
    "@types/sinon-chai": "^3.2.6",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1"
  }
}
